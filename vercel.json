{
    "buildCommand": "npm run build",
    "startCommand": "npm run start",
    "outputDirectory": "dist",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}